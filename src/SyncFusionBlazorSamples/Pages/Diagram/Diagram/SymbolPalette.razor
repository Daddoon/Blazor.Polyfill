@page "/diagram/symbol-palette"
@using Syncfusion.Blazor.Diagrams
@using System.Collections.ObjectModel
@using Syncfusion.Blazor.Buttons
@using Syncfusion.Blazor.Inputs
@using Syncfusion.Blazor.DropDowns

<SampleDescription>
    <p> This example illustrates predefining shapes in a palette that can be easily dragged into the drawing area. Customizable options of the symbol palette are also illustrated in this example.</p>
    <p> In this example, options to enable or disable animation, show or hide symbol descriptions, and show header icons are provided.</p>
    <p style="font-weight: bold;">A new blazor diagram component which provides better performance than the existing diagram control in Blazor WebAssembly App. It is available in preview mode. Check the samples <a target='_blank' href='diagramcomponent/flowchart'>here</a>.</p>
</SampleDescription>
<ActionDescription>
   <p>  This example shows how to add shapes to the symbol palette and how to customize it. The <a target='_blank' href='https://blazor.syncfusion.com/documentation/diagram/symbol-palette/'>Symbols</a> property can be used to add shapes to the symbol palette. The <a target='_blank' href='https://blazor.syncfusion.com/documentation/diagram/symbol-palette/#create-symbol-palette'>SymbolWidth</a> and <a target='_blank' href='https://blazor.syncfusion.com/documentation/diagram/symbol-palette/#create-symbol-palette'>SymbolHeight</a> properties allow you to define the size of the symbols.</p>
</ActionDescription>

@*Hidden:Lines*@
<div class="content-wrapper">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">

    <style>
        .property-panel-table {
            padding-left: 0px;
            font-weight: 600;
            margin-bottom: 10px;
            font-size: medium;
        }

        @@font-face {
            font-family: 'e-ddb-icons';
            src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
            font-weight: normal;
            font-style: normal;
        }

        .e-ddb-icons {
            font-family: 'e-ddb-icons';
            speak: none;
            font-size: 16px;
            font-style: normal;
            font-weight: normal;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .col-xs-6 {
            padding-left: 0px;
            padding-right: 0px;
            padding-top: 5px;
        }

        .texstyle {
            display: table;
            height: 35px;
            padding-right: 4px;
            padding-left: 0px;
            width: 50%;
            padding-top: 15px;
            float: left;
            position: relative;
            min-height: 1px;
        }

        .row {
            margin-left: 0px;
            margin-right: 0px;
            display: block;
        }

        .e-basic::before {
            content: "\e726";
        }

        .e-flow::before {
            content: "\e724";
        }

        .e-connector::before {
            content: "\e725";
        }

        .e-control e-symbolpalette e-lib e-draggable {
            display: block;
            width: 80%;
            margin: 0 auto;
            padding: 20px;
        }
    </style>
    @*End:Hidden*@
    @*Hidden:Lines*@
    <div class="col-lg-9 control-section" style="border-right: 1px solid #D7D7D7;padding-right: 0px;">
        @*End:Hidden*@
        <div id="palette-space" style="overflow: auto;height: 645px;margin-left: 15%;">
            <SfSymbolPalette ID="symbolPalette" @ref="@Palette" Width="80%" SymbolInfo="@SymbolInfo" Height="645px" Palettes="@Palettes" SymbolHeight="@SymbolHeight" SymbolWidth="@SymbolWidth" ExpandMode="@ExpandModeType" EnableAnimation="@AnimationMode" AllowDrag="false">
                <SymbolMargin Left="15" Right="15" Bottom="15" Top="15"></SymbolMargin>
            </SfSymbolPalette>
        </div>
        @*Hidden:Lines*@
    </div>
    <div class="col-lg-3 property-section">
        <div class="property-panel-header property-panel-table">
            Properties
        </div>
        <div class="row" style="padding-top: 8px">
            <div class="texstyle">
                Expandable:
            </div>
            <div class="col-xs-6">
                <SfDropDownList @ref="@Expandable" TValue="string" @bind-Index="@index" TItem="Modevalue" ID="expand" Width="100%" DataSource="@ExpandMode" Placeholder="Select Mode">
                    <DropDownListEvents TValue="string" TItem="Modevalue" ValueChange="ModeChange"></DropDownListEvents>
                    <DropDownListFieldSettings Text="Value" Value="Text"></DropDownListFieldSettings>
                </SfDropDownList>
            </div>
        </div>
        <div class="row" style="padding-top: 8px">
            <div class="texstyle">
                Symbol Size:
            </div>
            <div class="col-xs-6">
                <SfNumericTextBox TValue="int?" ID="size" Format="##.##" Value="@sizeChangeValue" Max="100" Min="50" Step="5" Width="100%">
                    <NumericTextBoxEvents TValue="int?" ValueChange="sizeChange"></NumericTextBoxEvents>
                </SfNumericTextBox>
            </div>
        </div>
        <div class="row" style="padding-top: 8px">
            <div class="texstyle">
                Animation:
            </div>
            <div class="col-xs-6" style="padding-top: 15px">
                <SfCheckBox ID="animation" Checked="@animationChecked" ValueChange="animationChange" TChecked="bool">

                </SfCheckBox>
            </div>
        </div>
        <div class="row" style="padding-top: 8px">
            <div class="texstyle">
                Header Icon:
            </div>
            <div class="col-xs-6" style="padding-top: 15px">
                <SfCheckBox ID="headericon" Checked="@icon" ValueChange="iconChange" TChecked="bool"></SfCheckBox>
            </div>
        </div>

    </div>
</div>
@*End:Hidden*@

@code{
    SfSymbolPalette Palette;
    private int? index { get; set; } = 1;
    public ObservableCollection<SymbolPalettePalette> Palettes;
    SfDropDownList<string, Modevalue> Expandable;

    public int? sizeChangeValue { get; set; } = 80;
    bool animationChecked = true;
    bool AnimationMode = true;

    bool icon = true;

    double SymbolHeight = 80;

    double SymbolWidth = 80;

    public string Height { get; set; } = "580px";

    public double length = 70;

    public ObservableCollection<DiagramConnector> ConnectorCollection
    {
        get;
        set;
    }


    public SymbolInfo SymbolInfo { get; set; }
    public ObservableCollection<object> BasicShape { get; set; }
    public ObservableCollection<object> Connectors { get; set; }
    public ObservableCollection<object> FlowShapes { get; set; }

    public class Modevalue
    {
        public string Text { get; set; }
        public string Value { get; set; }

    }

    private List<Modevalue> ExpandMode = new List<Modevalue>() {
         new Modevalue  { Text ="Single",  Value= "Single" },
         new Modevalue  { Text = "Multiple", Value = "Multiple" },
    };

    Syncfusion.Blazor.Navigations.ExpandMode ExpandModeType = Syncfusion.Blazor.Navigations.ExpandMode.Multiple;

    public void ModeChange(Syncfusion.Blazor.DropDowns.ChangeEventArgs<string, Modevalue> args)
    {
        if (args.Value == "Multiple")
        {
            this.ExpandModeType = Syncfusion.Blazor.Navigations.ExpandMode.Multiple;
        }
        else
        {
            this.ExpandModeType = Syncfusion.Blazor.Navigations.ExpandMode.Single;

        }
        this.StateHasChanged();
    }

    private void sizeChange(Syncfusion.Blazor.Inputs.ChangeEventArgs<int?> args)
    {
        this.SymbolWidth = double.Parse(args.Value.ToString());
        this.SymbolHeight = double.Parse(args.Value.ToString());
        this.sizeChangeValue = int.Parse(args.Value.ToString());
        this.StateHasChanged();
    }


    private void animationChange(Syncfusion.Blazor.Buttons.ChangeEventArgs<bool> args)
    {
        this.animationChecked = args.Checked;
        this.AnimationMode = args.Checked;
        this.StateHasChanged();
    }

    private void textChange(bool isChecked)
    {

    }


    private void iconChange(Syncfusion.Blazor.Buttons.ChangeEventArgs<bool> args)
    {
        for(var i = 0; i < Palettes.Count; i++)
        {
            this.icon = args.Checked;
            if (args.Checked)
            {
                if (i == 0)
                {
                    Palettes[i].IconCss = "e-ddb-icons e-basic";
                }
                else if (i == 1)
                {
                    Palettes[i].IconCss = "e-ddb-icons e-flow";
                }
                else if (i == 2)
                {
                    Palettes[i].IconCss = "e-ddb-icons e-connector";
                }
            }
            else
            {
                Palettes[i].IconCss = "";
            }
            this.StateHasChanged();
        }
    }

    protected override void OnInitialized()
    {
        InitializePalatte();
    }

    private void InitializePalatte()
    {
        Palettes = new ObservableCollection<SymbolPalettePalette>();

        SymbolInfo = new SymbolInfo()
        {
            Fit = true
        };

        BasicShape = new ObservableCollection<object>
        {
            new DiagramNode()
            {
                Id="Rectangle" , Shape = new DiagramShape(){
                Type = Syncfusion.Blazor.Diagrams.Shapes.Basic, BasicShape=BasicShapes.Rectangle},
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Ellipse" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Ellipse },
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Parallelogram" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Parallelogram },
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Triangle" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Triangle},
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Hexagon" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Hexagon},
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Pentagon" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Pentagon },
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Cylinder" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic,BasicShape=BasicShapes.Cylinder},
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
            new DiagramNode()
            {
                Id="Star" , Shape = new DiagramShape(){
                Type =Syncfusion.Blazor.Diagrams.Shapes.Basic, BasicShape=BasicShapes.Star},
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575"}
            },
        };

        Connectors = new ObservableCollection<object>()
        {
            new DiagramConnector(){Id="Link1" ,  Type = Syncfusion.Blazor.Diagrams.Segments.Orthogonal ,SourcePoint= new ConnectorSourcePoint(){ X=0,Y=0 },TargetPoint = new ConnectorTargetPoint(){  X=40,Y=40},
              TargetDecorator= new ConnectorTargetDecorator()
              { Shape=DecoratorShapes.Arrow, Style = new DecoratorShapeStyle() { StrokeColor = "#757575", Fill = "#757575" }},Style= new ConnectorShapeStyle(){ StrokeWidth=2, StrokeColor = "#757575"}
            },
            new DiagramConnector(){Id="link3" ,  Type=Syncfusion.Blazor.Diagrams.Segments.Orthogonal,SourcePoint= new ConnectorSourcePoint(){ X=0,Y=0 },TargetPoint = new ConnectorTargetPoint(){   X=40,Y=40},
             TargetDecorator= new ConnectorTargetDecorator(){ Shape=DecoratorShapes.None},Style= new ConnectorShapeStyle(){ StrokeWidth=2, StrokeColor = "#757575"}
            },
            new DiagramConnector(){Id="Link21" ,  Type=Syncfusion.Blazor.Diagrams.Segments.Straight,SourcePoint= new ConnectorSourcePoint(){ X=0,Y=0 },TargetPoint = new ConnectorTargetPoint(){  X=40,Y=40},
              TargetDecorator= new ConnectorTargetDecorator()
              { Shape=DecoratorShapes.Arrow, Style = new DecoratorShapeStyle() { StrokeColor = "#757575", Fill = "#757575" }},Style= new ConnectorShapeStyle(){ StrokeWidth=2, StrokeColor = "#757575"}
            },
            new DiagramConnector(){Id="link23" ,  Type=Syncfusion.Blazor.Diagrams.Segments.Straight,SourcePoint= new ConnectorSourcePoint(){ X=0,Y=0 },TargetPoint = new ConnectorTargetPoint(){  X=40,Y=40},
              TargetDecorator= new ConnectorTargetDecorator(){ Shape=DecoratorShapes.None},Style= new ConnectorShapeStyle(){ StrokeWidth=2, StrokeColor = "#757575"}
            },
            new DiagramConnector(){Id="link33" ,  Type=Syncfusion.Blazor.Diagrams.Segments.Bezier,SourcePoint= new ConnectorSourcePoint(){ X=0,Y=0 },TargetPoint = new ConnectorTargetPoint(){ X=40,Y=40},
              TargetDecorator= new ConnectorTargetDecorator(){ Shape=DecoratorShapes.None},Style= new ConnectorShapeStyle(){ StrokeWidth=2, StrokeColor = "#757575"}
            }
        };

        FlowShapes = new ObservableCollection<object>()
        {
            new DiagramNode()
            {
                Id = "Terminator",
                Style = new NodeShapeStyle(){StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.Terminator }
            },
            new DiagramNode()
            {
                Id = "Process",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.Process }
            },
            new DiagramNode()
            {
                Id = "Sort",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.Sort }
            },
            new DiagramNode()
            {
                Id = "Document",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.Document }
            },
            new DiagramNode()
            {
                Id = "Predefined Process",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.PreDefinedProcess }
            },
            new DiagramNode()
            {
                Id = "Punched Tape",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.PaperTap }
            },
            new DiagramNode()
            {
                Id = "Direct Data",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.DirectData
                }
            },
            new DiagramNode()
            {
                Id = "Sequential Data",
                Style = new NodeShapeStyle() { StrokeWidth = 2, StrokeColor = "#757575" },
                Shape = new DiagramShape() { Type = Syncfusion.Blazor.Diagrams.Shapes.Flow, FlowShape = Syncfusion.Blazor.Diagrams.FlowShapes.SequentialData }
            }
        };

        Palettes.Add(
            new SymbolPalettePalette()
            {
                Id = "Flowshapes",
                Expanded = true,
                Symbols = FlowShapes,
                Title = "Flowshapes",
                IconCss = "e-ddb-icons e-flow"
            }
        );
        Palettes.Add(
           new SymbolPalettePalette()
           {
               Id = "BasicShape",
               Expanded = true,
               Symbols = BasicShape,
               Title = "Basic Shapes",
               IconCss = "e-ddb-icons e-basic"
           }
        );

        Palettes.Add(
            new SymbolPalettePalette()
            {
                Id = "Connectors",
                Expanded = true,
                Symbols = Connectors,
                Title = "Connectors",
                IconCss = "e-ddb-icons e-connector"
            }
        );
    }
}
